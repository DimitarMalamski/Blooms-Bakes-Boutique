@model IEnumerable<FlowerServiceModel>

@{
	ViewBag.Title = "My Flowers";
}

<h2 class="text-center" style="padding-top: 2em; padding-bottom: 1.5em"><a style="color: #f075a0">My</a> Flowers</h2>

@if (Model.Any() == false)
{
	<h3 class="text-center">You have no flowers yet!</h3>
}

<div class="row">
	@foreach (var item in Model)
	{
		<partial name="_FlowerPartial" model="@item" />
	}
</div>